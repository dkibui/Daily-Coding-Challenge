   "source": [
    "def sum_without_plus_minus(a, b):\n",
    "    while b != 0:\n",
    "        carry = a & b\n",
    "        a = a ^ b\n",
    "        b = carry << 1\n",
    "    return a\n",
    "\n",
    "sum_without_plus_minus(2, 4)"
   ]
