   "source": [
    "def find_sum_of_two(A, val):\n",
    "    for i, num in enumerate(A):\n",
    "        target = val - num\n",
    "        if target in A[i+1:]:\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "A = [2,61,4,-53]\n",
    "val = 6\n",
    "find_sum_of_two(A, val)"
