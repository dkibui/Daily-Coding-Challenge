   "source": [
    "def row_sum_odd_numbers(n):\n",
    "    starting = 1\n",
    "    for i in range(2, n + 1):\n",
    "        starting += 2 * (i - 1)\n",
    "    return sum(range(starting, starting + (2 * n), 2))"
   ]
  }
